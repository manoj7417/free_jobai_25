{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "app/api/**/*.js": {
      "maxDuration": 30
    }
  },
  "build": {
    "env": {
      "NPM_FLAGS": "--legacy-peer-deps --force"
    }
  },
  "installCommand": "npm install --legacy-peer-deps"
}
